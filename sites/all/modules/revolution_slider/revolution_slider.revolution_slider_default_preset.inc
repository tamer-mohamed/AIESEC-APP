<?php

/**
 * @file
 * Bulk export of default_revolution_slider_presets objects generated by Bulk export module.
 */

/**
 * Implements hook_revolution_slider_default_presets().
 */
function revolution_slider_revolution_slider_default_presets() {
  $presets = array();

  $preset = revolution_slider_optionset_create();
  $preset->disabled = FALSE; /* Edit this to true to make a default preset disabled initially */
  $preset->api_version = 1;
  $preset->name = 'default';
  $preset->title = 'Default';
  $preset->imagestyle_normal = 'revolution_slider_full';
  $preset->imagestyle_thumbnail = 'revolution_slider_thumbnail';
  $presets['default'] = $preset;

  return $presets;
}
